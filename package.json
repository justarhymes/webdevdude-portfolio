{
  "name": "webdevdude-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create:demo": "tsx scripts/create-demo.ts",
    "create:resume": "tsx -r tsconfig-paths/register scripts/create-resume-item.ts",
    "peek:project": "tsx -r tsconfig-paths/register scripts/peek-project.ts",
    "migrate:slugs": "tsx scripts/backfill-slugs.ts",
    "migrate:test": "tsx scripts/migrate-test-to-prod.ts",
    "seed": "tsx -r tsconfig-paths/register scripts/seed.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --reporter=verbose",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "e2e": "playwright test --config=playwright.config.ts",
    "e2e:ui": "playwright test --ui --config=playwright.config.ts"
  },
  "dependencies": {
    "mongoose": "^8.16.4",
    "next": "15.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "mongodb-memory-server": "^10.2.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
